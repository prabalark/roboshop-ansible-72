[Unit]
Description = Catalogue Service

[Service]
User=roboshop
Environment={{ lookup('aws_ssm','{{env}}.frontend.catalogue_montru',region='us-east-1') }}
Environment=MONGO_URL="{{ lookup('aws_ssm','{{env}}.frontend.catalogue_monurl',region='us-east-1') }}"
ExecStart=/bin/node /app/server.js
SyslogIdentifier=catalogue

[Install]
WantedBy=multi-user.target